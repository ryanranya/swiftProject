# platform :ios, '9.0'

target 'TruckBang' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
	#tabbar
	pod 'ESTabBarController-swift'
    
    #网络请求
    pod 'Moya'
    
    #自动布局
    pod 'SnapKit'
    
    #图片处理
    pod 'Kingfisher'
    
    #模型转换
    pod 'HandyJSON'
    
  # Pods for TruckBang

  target 'TruckBangTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'TruckBangUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
